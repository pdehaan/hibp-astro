{
  "name": "hibp-astro",
  "description": "Testing Astro routes and stuff w/ HIBP API.",
  "version": "0.0.1",
  "author": "Peter deHaan",
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "astro": "^5.14.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "prettier-plugin-astro": "^0.14.1"
  },
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check",
    "dev": "astro dev",
    "preview": "astro preview",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "type": "module",
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
